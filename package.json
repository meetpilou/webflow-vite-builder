{
  "name": "vite-starter",
  "private": true,
  "version": "0.0.2",
  "type": "module",
  "scripts": {
    "setup": "node ./scripts/setup.js",
    "dev": "node ./scripts/watch.js",
    "build": "node ./scripts/build.js patch",
    "build:minor": "node ./scripts/build.js minor",
    "build:major": "node ./scripts/build.js major",
    "deploy:staging": "node ./scripts/deploy.js staging",
    "deploy:prod": "node ./scripts/deploy.js prod",
    "restore": "node ./scripts/restore.js",
    "reset": "node ./scripts/reset.js",
    "snippet": "node ./scripts/snippet.js",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.0.7",
    "eslint": "8",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.0",
    "postcss-nested": "^7.0.2",
    "prettier": "^3.6.2",
    "sass": "^1.89.2",
    "vite": "^7.2.2"
  },
  "dependencies": {
    "fs-extra": "^11.3.2",
    "semver": "^7.7.3"
  }
}
